
static class Task implements Runnable {

	//public static int counter = 1;
	public static void main(String[] args) {
		Thread thread1 = new Thread(new Task());
		thread1.start();
		System.out.println("Thread Name: "+Thread.currentThread().getName());
	}
	
	@Override
	public void run() {
			System.out.println("Thread Name: "+Thread.currentThread().getName());
			//counter = counter + 1;
	}

}
